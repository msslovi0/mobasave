                            </div>
                        </div>
                        <div class="sm:col-span-3">
                            <label for="form_youtube" class="block text-sm/6 font-medium text-gray-900 dark:text-white">{{ 'manufacturer.field.youtube'|trans }}</label>
                            <div>
                                {{ form_widget(manufacturerform.youtube, {'attr': {'class': 'col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-8 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700'}}) }}
                            </div>
                        </div>
                        <div class="sm:col-span-3">
                            <label for="form_twitter" class="block text-sm/6 font-medium text-gray-900 dark:text-white">{{ 'manufacturer.field.twitter'|trans }}</label>
                            <div>
                                {{ form_widget(manufacturerform.twitter, {'attr': {'class': 'col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-8 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700'}}) }}
                            </div>
                        </div>
                        <div class="sm:col-span-3">
                            <label for="form_tiktok" class="block text-sm/6 font-medium text-gray-900 dark:text-white">{{ 'manufacturer.field.tiktok'|trans }}</label>
                            <div>
                                {{ form_widget(manufacturerform.tiktok, {'attr': {'class': 'col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-8 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700'}}) }}
                            </div>
                        </div>
                        <div class="sm:col-span-3">
                            <label for="form_linkedin" class="block text-sm/6 font-medium text-gray-900 dark:text-white">{{ 'manufacturer.field.linkedin'|trans }}</label>
                            <div>
                                {{ form_widget(manufacturerform.linkedin, {'attr': {'class': 'col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-8 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700'}}) }}
                            </div>
