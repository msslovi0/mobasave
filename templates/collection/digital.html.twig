{% extends 'base.html.twig' %}
{% block title %}{{ 'global.nav.digital'|trans }} {{ model.name }} ({{ model.model }}){% endblock %}
{% block body %}
    <div class="py-10">
        <header>
            <div class="flex justify-between mx-auto max-w-10xl px-4 sm:px-6 lg:px-8">
                <h1 class="text-3xl font-bold tracking-tight text-gray-900 dark:text-white">{{ model.name }}</h1>
                <div class="flex gap-1">
                    <a href="{{ path('mbs_model_add') }}" type="button" class="block rounded-md bg-green-800 px-3 py-2 text-center text-sm font-semibold text-white shadow-xs hover:bg-green-700 focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-green-800 dark:bg-green-700 dark:hover:bg-green-600 dark:focus-visible:outline-green-700">{{ 'model.list.add'|trans }}</a>
                    <a href="{{ path('mbs_database', {id: app.session.get('database')}) }}" type="button" class="block rounded-md bg-white px-3 py-2 text-sm font-semibold text-gray-900 shadow-xs inset-ring inset-ring-gray-300 hover:bg-gray-50 dark:bg-white/10 dark:text-white dark:shadow-none dark:inset-ring-white/5 dark:hover:bg-white/20">{{ 'global.back'|trans }}</a>
                </div>
            </div>
        </header>
        <main>
            {{ form_start(digitalform, {'attr': {'class': 'mx-auto max-w-10xl px-4 py-8 sm:px-6 lg:px-8'}}) }}
            <div class="space-y-12">
                {%  include 'collection/nav.html.twig' with {'template': 'digital'} %}
                <div class="grid grid-cols-1 gap-x-8 gap-y-5 border-b border-gray-900/10 md:grid-cols-3 dark:border-white/10">
                    <div class="row-span-2">
                        {% if model.image!="" %}<img src="{{ asset('data/image/' ~ model.image) }}" alt="{{ model.name }}" class="mx-auto h-auto w-200" />{% endif %}
                    </div>

                    <div class="grid max-w-2xl grid-cols-2 gap-2 sm:grid-cols-6 md:col-span-2">
                        <div class="col-span-full">
                            {% for message in app.flashes('success') %}
                                <div class="border-l-4 border-green-400 bg-green-50 p-4 dark:border-green-500 dark:bg-green-500/10">
                                    <div class="flex">
                                        <div class="shrink-0">
                                            <svg viewBox="0 0 20 20" fill="currentColor" data-slot="icon" aria-hidden="true" class="size-5 text-green-400">
                                                <path d="M10 18a8 8 0 1 0 0-16 8 8 0 0 0 0 16Zm3.857-9.809a.75.75 0 0 0-1.214-.882l-3.483 4.79-1.88-1.88a.75.75 0 1 0-1.06 1.061l2.5 2.5a.75.75 0 0 0 1.137-.089l4-5.5Z" clip-rule="evenodd" fill-rule="evenodd" />
                                            </svg>
                                        </div>
                                        <div class="ml-3">
                                            <p class="text-sm text-green-700 dark:text-green-300">{{ message }}</p>
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                        </div>
                        <div class="sm:col-span-3">
                            <label for="form_address" class="block text-sm/6 font-medium text-gray-900 dark:text-white">{{ 'model.field.address'|trans }}</label>
                            <div class="grid grid-cols-1">
                                {{ form_widget(digitalform.address, {'attr': {'class': 'col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-8 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700'}}) }}
                            </div>
                        </div>
                        <div class="sm:col-span-3">
                            <label for="form_protocol" class="block text-sm/6 font-medium text-gray-900 dark:text-white">{{ 'model.field.protocol'|trans }}</label>
                            <div class="grid grid-cols-10 gap-2">
                                <div class="grid grid-cols-1 col-span-9">
                                    {{ form_widget(digitalform.protocol, {'attr': {'class': 'col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-8 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700'}}) }}
                                    <svg viewBox="0 0 16 16" fill="currentColor" data-slot="icon" aria-hidden="true" class="pointer-events-none col-start-1 row-start-1 mr-2 size-5 self-center justify-self-end text-gray-500 sm:size-4 dark:text-gray-400">
                                        <path d="M4.22 6.22a.75.75 0 0 1 1.06 0L8 8.94l2.72-2.72a.75.75 0 1 1 1.06 1.06l-3.25 3.25a.75.75 0 0 1-1.06 0L4.22 7.28a.75.75 0 0 1 0-1.06Z" clip-rule="evenodd" fill-rule="evenodd" />
                                    </svg>
                                </div>
                                <button type="button" command="show-modal" class="add-value" commandfor="drawer" data-title="{{ 'model.field.protocol'|trans }}" data-label="{{ 'model.field.new'|trans({'name': 'model.field.protocol'|trans}) }}" data-entity="protocol" class="flex shrink-0 items-end bg-white text-base text-gray-500 sm:text-sm/6 dark:bg-white/5 dark:text-gray-400 dark:outline-gray-700">
                                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="pointer-events-none col-start-1 row-start-1 w-10 h-10 self-center justify-self-end text-gray-500 sm:size-5 dark:text-gray-400">
                                        <path stroke-linecap="round" stroke-linejoin="round" d="m16.862 4.487 1.687-1.688a1.875 1.875 0 1 1 2.652 2.652L10.582 16.07a4.5 4.5 0 0 1-1.897 1.13L6 18l.8-2.685a4.5 4.5 0 0 1 1.13-1.897l8.932-8.931Zm0 0L19.5 7.125M18 14v4.75A2.25 2.25 0 0 1 15.75 21H5.25A2.25 2.25 0 0 1 3 18.75V8.25A2.25 2.25 0 0 1 5.25 6H10" />
                                    </svg>
                                </button>
                                <span class="sr-only">{{ 'model.list.edit'|trans }}</span>
                            </div>
                        </div>
                        <div class="sm:col-span-3">
                            <label for="form_decoder" class="block text-sm/6 font-medium text-gray-900 dark:text-white">{{ 'model.field.decoder'|trans }}</label>
                            <div class="grid grid-cols-10 gap-2">
                                <div class="grid grid-cols-1 col-span-9">
                                    {{ form_widget(digitalform.decoder, {'attr': {'class': 'col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-8 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700'}}) }}
                                    <svg viewBox="0 0 16 16" fill="currentColor" data-slot="icon" aria-hidden="true" class="pointer-events-none col-start-1 row-start-1 mr-2 size-5 self-center justify-self-end text-gray-500 sm:size-4 dark:text-gray-400">
                                        <path d="M4.22 6.22a.75.75 0 0 1 1.06 0L8 8.94l2.72-2.72a.75.75 0 1 1 1.06 1.06l-3.25 3.25a.75.75 0 0 1-1.06 0L4.22 7.28a.75.75 0 0 1 0-1.06Z" clip-rule="evenodd" fill-rule="evenodd" />
                                    </svg>
                                </div>
                                <button type="button" command="show-modal" class="add-value" data-title="{{ 'model.field.decoder'|trans }}" data-label="{{ 'model.field.new'|trans({'name': 'model.field.decoder'|trans}) }}" data-entity="decoder" commandfor="drawer" class="flex shrink-0 items-end bg-white text-base text-gray-500 sm:text-sm/6 dark:bg-white/5 dark:text-gray-400 dark:outline-gray-700">
                                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="pointer-events-none col-start-1 row-start-1 w-10 h-10 self-center justify-self-end text-gray-500 sm:size-5 dark:text-gray-400">
                                        <path stroke-linecap="round" stroke-linejoin="round" d="m16.862 4.487 1.687-1.688a1.875 1.875 0 1 1 2.652 2.652L10.582 16.07a4.5 4.5 0 0 1-1.897 1.13L6 18l.8-2.685a4.5 4.5 0 0 1 1.13-1.897l8.932-8.931Zm0 0L19.5 7.125M18 14v4.75A2.25 2.25 0 0 1 15.75 21H5.25A2.25 2.25 0 0 1 3 18.75V8.25A2.25 2.25 0 0 1 5.25 6H10" />
                                    </svg>
                                </button>
                                <span class="sr-only">{{ 'model.list.edit'|trans }}</span>
                            </div>
                        </div>
                        <div class="sm:col-span-3">
                            <label for="form_pininterface" class="block text-sm/6 font-medium text-gray-900 dark:text-white">{{ 'model.field.pininterface'|trans }}</label>
                            <div class="grid grid-cols-10 gap-2">
                                <div class="grid grid-cols-1 col-span-9">
                                    {{ form_widget(digitalform.pininterface, {'attr': {'class': 'col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-8 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700'}}) }}
                                    <svg viewBox="0 0 16 16" fill="currentColor" data-slot="icon" aria-hidden="true" class="pointer-events-none col-start-1 row-start-1 mr-2 size-5 self-center justify-self-end text-gray-500 sm:size-4 dark:text-gray-400">
                                        <path d="M4.22 6.22a.75.75 0 0 1 1.06 0L8 8.94l2.72-2.72a.75.75 0 1 1 1.06 1.06l-3.25 3.25a.75.75 0 0 1-1.06 0L4.22 7.28a.75.75 0 0 1 0-1.06Z" clip-rule="evenodd" fill-rule="evenodd" />
                                    </svg>
                                </div>
                                <button type="button" command="show-modal" class="add-value" data-title="{{ 'model.field.pininterface'|trans }}" data-label="{{ 'model.field.new'|trans({'name': 'model.field.pininterface'|trans}) }}" data-entity="pininterface" commandfor="drawer" class="flex shrink-0 items-end bg-white text-base text-gray-500 sm:text-sm/6 dark:bg-white/5 dark:text-gray-400 dark:outline-gray-700">
                                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="pointer-events-none col-start-1 row-start-1 w-10 h-10 self-center justify-self-end text-gray-500 sm:size-5 dark:text-gray-400">
                                        <path stroke-linecap="round" stroke-linejoin="round" d="m16.862 4.487 1.687-1.688a1.875 1.875 0 1 1 2.652 2.652L10.582 16.07a4.5 4.5 0 0 1-1.897 1.13L6 18l.8-2.685a4.5 4.5 0 0 1 1.13-1.897l8.932-8.931Zm0 0L19.5 7.125M18 14v4.75A2.25 2.25 0 0 1 15.75 21H5.25A2.25 2.25 0 0 1 3 18.75V8.25A2.25 2.25 0 0 1 5.25 6H10" />
                                    </svg>
                                    <span class="sr-only">{{ 'model.list.edit'|trans }}</span>
                                </button>
                            </div>
                        </div>
                    </div>
                    {% if functions|length>0 %}
                    <div class="grid max-w-2xl grid-cols-2 gap-2 sm:grid-cols-6 md:col-span-2">
                        <div class="sm:col-span-3">
                            <span for="form_address" class="block text-sm/6 font-medium text-gray-900 dark:text-white">{{ 'model.field.functions'|trans }}</span>
                        </div>
                        <div class="col-span-full">
                            <div class="flow-root">
                                <div class="-mx-4 -my-2 overflow-x-auto sm:-mx-6 lg:-mx-8">
                                    <div class="inline-block min-w-full py-2 align-middle sm:px-6 lg:px-8">
                                        <table id="functions" class="relative min-w-full divide-y divide-gray-300 dark:divide-white/15">
                                        <thead>
                                            <tr>
                                                <th scope="col" class="py-3.5 pr-3 pl-4 text-left text-sm font-semibold whitespace-nowrap text-gray-900 sm:pl-0 dark:text-white">{{'model.field.functionkey'|trans}}</th>
                                                <th scope="col" class="px-2 py-3.5 text-left text-sm font-semibold whitespace-nowrap text-gray-900 dark:text-white">{{'model.field.function'|trans}}</th>
                                                <th scope="col" class="py-3.5 pr-4 pl-3 whitespace-nowrap sm:pr-0">
                                                    <span class="sr-only">{{ 'model.list.sound'|trans }}</span>
                                                </th>
                                                <th scope="col" class="py-3.5 pr-4 pl-3 whitespace-nowrap sm:pr-0">
                                                    <span class="sr-only">{{ 'model.list.light'|trans }}</span>
                                                </th>
                                                <th scope="col" class="py-3.5 pr-4 pl-3 whitespace-nowrap sm:pr-0">
                                                    <span class="sr-only">{{ 'model.list.edit'|trans }}</span>
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        {% for key in functions|keys %}
                                            {% for function in functions[key] %}
                                            <tr id="digitalfunction-{{ function.id }}">
                                                <td class="py-2 pr-3 pl-4 text-sm whitespace-nowrap text-gray-500 sm:pl-0 dark:text-gray-400">{{ function.key }}</td>
                                                <td class="px-2 py-2 text-sm whitespace-nowrap text-gray-900 dark:text-white">{{ function.name }}</td>
                                                <td class="px-2 py-2 text-sm whitespace-nowrap text-gray-900 dark:text-white">
                                                    {% if function.sound==1 %}
                                                    <button type="button" class="">
                                                        <svg fill="none" stroke="currentColor" class="pointer-events-none col-start-1 row-start-1 mr-2 size-5 self-center justify-self-end text-gray-500 sm:size-4 dark:text-gray-400" stroke-width="1.5" viewBox="0 0 24 24" version="1.1" xmlns="http://www.w3.org/2000/svg">
                                                            <path stroke-linecap="round" stroke-linejoin="round" d="M19.114 5.636a9 9 0 0 1 0 12.728M16.463 8.288a5.25 5.25 0 0 1 0 7.424M6.75 8.25l4.72-4.72a.75.75 0 0 1 1.28.53v15.88a.75.75 0 0 1-1.28.53l-4.72-4.72H4.51c-.88 0-1.704-.507-1.938-1.354A9.009 9.009 0 0 1 2.25 12c0-.83.112-1.633.322-2.396C2.806 8.756 3.63 8.25 4.51 8.25H6.75Z" />
                                                        </svg><span class="sr-only">{{ 'model.function.sound'|trans }}</span>
                                                    </button>
                                                    {% endif %}
                                                </td>
                                                <td class="px-2 py-2 text-sm whitespace-nowrap text-gray-900 dark:text-white">
                                                    {% if function.light==1 %}
                                                    <button type="button" class="align-center">
                                                        <svg class="pointer-events-none col-start-1 row-start-1 mr-2 size-5 self-center justify-self-end text-gray-500 sm:size-4 dark:text-gray-400" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                                                            <path stroke-linecap="round" stroke-linejoin="round" d="M12 18v-5.25m0 0a6.01 6.01 0 0 0 1.5-.189m-1.5.189a6.01 6.01 0 0 1-1.5-.189m3.75 7.478a12.06 12.06 0 0 1-4.5 0m3.75 2.383a14.406 14.406 0 0 1-3 0M14.25 18v-.192c0-.983.658-1.823 1.508-2.316a7.5 7.5 0 1 0-7.517 0c.85.493 1.509 1.333 1.509 2.316V18" />
                                                        </svg><span class="sr-only">{{ 'model.function.light'|trans }}</span>
                                                    </button>
                                                    {% endif %}
                                                </td>
                                                <td class="px-2 py-2 text-sm whitespace-nowrap text-gray-900 dark:text-white">
                                                <button type="button" command="show-modal" commandfor="dialog" class="delete-function" data-headline="{{ 'model.function.delete.headline'|trans({'function': function.key ~ " " ~ function.name}) }}" data-path="{{ path('mbs_model_function_delete', {'id': function.id}) }}">
                                                    <svg class="pointer-events-none col-start-1 row-start-1 mr-2 size-5 self-center justify-self-end text-gray-500 sm:size-4 dark:text-gray-400 stroke-red-600 dark:stroke-red-500" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                                                        <path stroke-linecap="round" stroke-linejoin="round" d="m14.74 9-.346 9m-4.788 0L9.26 9m9.968-3.21c.342.052.682.107 1.022.166m-1.022-.165L18.16 19.673a2.25 2.25 0 0 1-2.244 2.077H8.084a2.25 2.25 0 0 1-2.244-2.077L4.772 5.79m14.456 0a48.108 48.108 0 0 0-3.478-.397m-12 .562c.34-.059.68-.114 1.022-.165m0 0a48.11 48.11 0 0 1 3.478-.397m7.5 0v-.916c0-1.18-.91-2.164-2.09-2.201a51.964 51.964 0 0 0-3.32 0c-1.18.037-2.09 1.022-2.09 2.201v.916m7.5 0a48.667 48.667 0 0 0-7.5 0" />
                                                    </svg><span class="sr-only">{{ 'model.function.trash'|trans({'function': function.key ~ " " ~ function.name}) }}</span>
                                                </button>
                                                </td>
                                            </tr>
                                            {% endfor %}
                                        {% endfor %}
                                        </tbody>
                                        <tfoot>
                                        <tr>
                                            <td class="py-2 pr-3 pl-4 text-sm whitespace-nowrap text-gray-500 sm:pl-0 dark:text-gray-400">
                                                <div class="grid grid-cols-1 mr-2">
                                                    <select name="add-key" id="add-key" class="col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-1 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700">
                                                        <option></option>
                                                        {% for key in functionkey %}
                                                            <option value="{{ key.id }}">{{ key.name }}</option>
                                                        {% endfor %}

                                                    </select>
                                                    <svg viewBox="0 0 16 16" fill="currentColor" data-slot="icon" aria-hidden="true" class="pointer-events-none col-start-1 row-start-1 mr-2 size-5 self-center justify-self-end text-gray-500 sm:size-4 dark:text-gray-400">
                                                        <path d="M4.22 6.22a.75.75 0 0 1 1.06 0L8 8.94l2.72-2.72a.75.75 0 1 1 1.06 1.06l-3.25 3.25a.75.75 0 0 1-1.06 0L4.22 7.28a.75.75 0 0 1 0-1.06Z" clip-rule="evenodd" fill-rule="evenodd" />
                                                    </svg>
                                                </div>
                                            </td>
                                            <td class="px-2 py-2 text-sm whitespace-nowrap text-gray-900 dark:text-white">
                                                <div class="grid grid-cols-1 mr-2">
                                                    <input name="add-decoderfunction" id="add-decoderfunction" type="text" class="col-start-1 row-start-1 w-full appearance-none rounded-md bg-white py-1.5 pr-8 pl-3 text-base text-gray-900 outline-1 -outline-offset-1 outline-gray-300 focus:outline-2 focus:-outline-offset-2 focus:outline-green-800 sm:text-sm/6 dark:bg-white/5 dark:text-white dark:outline-white/10 dark:*:bg-gray-800 dark:focus:outline-green-700"/>
                                                </div>
                                            </td>
                                            <td class="px-2 py-2 text-sm whitespace-nowrap text-gray-900 dark:text-white">
                                                <div class="group grid size-4 grid-cols-1">
                                                    <input id="add-sound" type="checkbox" name="add-sound" value="1" aria-describedby="comments-description" class="col-start-1 row-start-1 appearance-none rounded-sm border border-gray-300 bg-white checked:border-green-800 checked:bg-green-800 indeterminate:border-green-800 indeterminate:bg-green-800 focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-green-800 disabled:border-gray-300 disabled:bg-gray-100 disabled:checked:bg-gray-100 dark:border-white/10 dark:bg-white/5 dark:checked:border-green-700 dark:checked:bg-green-700 dark:indeterminate:border-green-700 dark:indeterminate:bg-green-700 dark:focus-visible:outline-green-700 dark:disabled:border-white/5 dark:disabled:bg-white/10 dark:disabled:checked:bg-white/10 forced-colors:appearance-auto" />
                                                    <svg viewBox="0 0 14 14" fill="none" class="pointer-events-none col-start-1 row-start-1 size-3.5 self-center justify-self-center stroke-white group-has-disabled:stroke-gray-950/25 dark:group-has-disabled:stroke-white/25">
                                                        <path d="M3 8L6 11L11 3.5" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="opacity-0 group-has-checked:opacity-100" />
                                                        <path d="M3 7H11" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="opacity-0 group-has-indeterminate:opacity-100" />
                                                    </svg>
                                                </div>
                                            </td>
                                            <td class="px-2 py-2 text-sm whitespace-nowrap text-gray-900 dark:text-white">
                                                <div class="group grid size-4 grid-cols-1">
                                                    <input id="add-light" type="checkbox" name="add-light" value="1" aria-describedby="comments-description" class="col-start-1 row-start-1 appearance-none rounded-sm border border-gray-300 bg-white checked:border-green-800 checked:bg-green-800 indeterminate:border-green-800 indeterminate:bg-green-800 focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-green-800 disabled:border-gray-300 disabled:bg-gray-100 disabled:checked:bg-gray-100 dark:border-white/10 dark:bg-white/5 dark:checked:border-green-700 dark:checked:bg-green-700 dark:indeterminate:border-green-700 dark:indeterminate:bg-green-700 dark:focus-visible:outline-green-700 dark:disabled:border-white/5 dark:disabled:bg-white/10 dark:disabled:checked:bg-white/10 forced-colors:appearance-auto" />
                                                    <svg viewBox="0 0 14 14" fill="none" class="pointer-events-none col-start-1 row-start-1 size-3.5 self-center justify-self-center stroke-white group-has-disabled:stroke-gray-950/25 dark:group-has-disabled:stroke-white/25">
                                                        <path d="M3 8L6 11L11 3.5" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="opacity-0 group-has-checked:opacity-100" />
                                                        <path d="M3 7H11" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="opacity-0 group-has-indeterminate:opacity-100" />
                                                    </svg>
                                                </div>
                                            </td>
                                            <td class="px-2 py-2 text-sm whitespace-nowrap text-gray-900 dark:text-white">
                                                <div class="grid grid-cols">
                                                    <input type="hidden" id="add-model" name="model" value="{{ model.id }}"/>
                                                    <input type="hidden" id="add-path" name="path" value="{{ path('mbs_model_function_add') }}"/>
                                                    <button type="button" id="add-function" class="rounded-md bg-green-800 py-2 text-sm font-semibold text-white shadow-xs hover:bg-green-700 focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-green-800 dark:bg-green-700 dark:shadow-none dark:focus-visible:outline-green-700">{{ 'model.function.add'|trans }}</button>
                                                </div>
                                            </td>
                                        </tr>
                                        </tfoot>
                                        </table>
                                    </div>
